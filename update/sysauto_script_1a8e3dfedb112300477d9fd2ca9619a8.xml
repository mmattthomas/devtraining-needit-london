        <run_start>2018-10-25 20:10:45</run_start>
        <run_time>1970-01-01 17:00:00</run_time>
        <run_type>daily</run_type>
        <script><![CDATA[var rightnow = new GlideDateTime();

var gr = new GlideRecord('x_58872_needit_needit_task');
gr.addQuery('due_date', '>=', rightnow);
gr.addQuery('due_date', '<=', gs.hoursAgo(-24));
gr.addQuery('state', '!=', 3);

gr.query();

while (gr.next()) {
	gs.info("NeedIT task due soon record = " + gr.number);
	gs.eventQueue('x_58872_needit.dueSoonNeedItTask', gr, gr.number, gs.getUserName());
}]]></script>
